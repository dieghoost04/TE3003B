<?xml version="1.0"?> <!--Declare XML Version  

typical value "1.0"--> 

<robot name="robot_sim"> 

    <!-- Enlace del chasis -->
    <link name="chassis"> 
        <visual>

            <origin xyz="0 0 0.05" rpy="1.57079632679 0 1.57079632679"/>
            <geometry>
                <mesh filename="package://puzzlebot_sim/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        
        </visual>
        
        <collision>
            <origin xyz="0 0 0.05" rpy="1.507 0 0"/>
            <geometry>
                <mesh filename="package://puzzlebot_sim/models/MCR2_1000_13_Chassis.stl"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <mass value="0.01"/>
            <inertia ixx="7.58333333333e-06" ixy="0.0" ixz="0.0" iyy="7.58333333333e-06" iyz="0.0" izz="1.25e-05"/>
        </inertial>
    
    </link> 

    <link name="wheel_left"> 

        <visual> 

            <origin xyz="0.055 0 0.056" rpy="0 0 1.57079632679"/> 

            <geometry>
                <mesh filename="package://puzzlebot_sim/models/Wheel.stl"/>
            </geometry>

        </visual> 

    </link> 

    

    <joint name="joint_base_to_robot" type="continuous"> 

        <parent link="chassis"/> 

        <!--parent: Parent link name --> 

        <child link="wheel_left"/> 

        <!--child: child link name --> 

        <origin xyz="0.0 0.15 0.05" rpy="1.5708 0 0" /> 

        <!--origin: This is the transform from the parent link to the child link 

        xyz: xyz offset rpy:rotaton offset (radians)--> 

        <axis xyz="1 0 0 " /> 

        <!--Rotation/Translation axis for the joints(revolute,continuos/prismatic) 

        noy used with Fixed Joints --> 

    </joint> 

    <link name="wheel_right"> 

        <visual> 

            <origin xyz="0.055 0 0.245" rpy="0 3.1416 1.57079632679"/> 

            <geometry>
                <mesh filename="package://puzzlebot_sim/models/Wheel.stl"/>
            </geometry>

        </visual> 

    </link> 

    

    <joint name="joint_base_to_robot_right" type="continuous"> 

        <parent link="chassis"/> 

        <!--parent: Parent link name --> 

        <child link="wheel_right"/> 

        <!--child: child link name --> 

        <origin xyz="0.0 0.15 0.05" rpy="1.5708 0 0" /> 

        <!--origin: This is the transform from the parent link to the child link 

        xyz: xyz offset rpy:rotaton offset (radians)--> 

        <axis xyz="1 0 0 " /> 

        <!--Rotation/Translation axis for the joints(revolute,continuos/prismatic) 

        noy used with Fixed Joints --> 

    </joint> 

</robot>
